   "source": [
    "# Crie um programa para imprimir de 1 até o número digitado pelo usuário, mas apenas os números impares."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "contador = 1\n",
    "numero_escolhido = int(input('Digite o último número a imprimir: '))\n",
    "while contador <= numero_escolhido:\n",
    "    print(contador)\n",
    "    contador = contador + 2"
   ]
  }
